#        s1=Select(driver.find_element_by_id('continents'))
        elements = driver.find_element_by_class_name('btn')
 
#        for opt in s1.options:
#            s1.select_by_visible_text('Europe')
        for Y in elements:
            print (Y)
 
    def tearDown(self):
        self.driver.quit()
 
 
 
 
 
